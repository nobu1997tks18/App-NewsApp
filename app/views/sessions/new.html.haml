.signup
  =render 'shared/header'
  .signup__title
    %h1 Login page
  .signup__form
    =form_with scope: :session, url: login_path, local: true do |f|
      -# = render 'shared/errors_messages'
      .form.form-name
        .form.form-email
          =f.label :email
          =f.email_field :email
        .form.form-password
          =f.label :password
          =f.password_field :password
        .form.form-submit
          =f.submit "送信" 
